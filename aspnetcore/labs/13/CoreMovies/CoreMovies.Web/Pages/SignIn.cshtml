@page
@model CoreMovies.Pages.SignInModel
@{
    ViewData["Title"] = "SignIn";
}

<h1>Sign In</h1>

<div>Hello!</div>

<table>
    @foreach (var identity in User.Identities)
    {
        <tr>
            <td>
                <table>
                    @foreach(var claim in identity.Claims)
                    {
                        <tr>
                            <td>
                                @claim.Type
                            </td>
                            <td>
                                @claim.Value
                            </td>
                        </tr>
                    }
                </table>
            </td>
        </tr>
    }
</table>

